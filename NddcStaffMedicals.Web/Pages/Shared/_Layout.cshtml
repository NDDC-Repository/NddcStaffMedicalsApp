                name: "Las Vegas: 275"
            },
            {
                coords: [32.77, -96.79],
                name: "Dallas: 225"
            }
            ];
            var map = new jsVectorMap({
                map: "us_aea_en",
                selector: "#usa_map",
                zoomButtons: true,
                markers: markers,
                markerStyle: {
                    initial: {
                        "r": 9,
                        "fill": window.theme.primary,
