                    }
                },
                regionStyle: {
                    initial: {
                        fill: "#DCE3E8"
                    }
                },
                zoomOnScroll: false
            });
            window.addEventListener("resize", () => {
                map.updateSize();
            });
            setTimeout(function () {
                map.updateSize();
            }, 250);
        });
    </script>
